/** "Xor" chip implementation from Nand2Tetris course
/* Website: www.nand2tetris.org  */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
        Nand(a=a, b=b, out=tmp1);
        Nand(a=a, b=a, out=nota);
        Nand(a=b, b=b, out=notb);
        Nand(a=nota, b=notb, out=tmp2);
        Nand(a=tmp1, b=tmp2, out=tmp3);
        Nand(a=tmp3, b=tmp3, out=out);
}
